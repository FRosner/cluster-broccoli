{
    "Job": {
        "Region": "global",
        "ID": "{{id}}",
        "Name": "{{id}}",
        "Type": "service",
        "Priority": 50,
        "AllAtOnce": false,
        "Datacenters": [
            "dc1"
        ],
        "Constraints": [
            {
                "LTarget": "${attr.kernel.name}",
                "RTarget": "linux",
                "Operand": "distinct_hosts"
            }
        ],
        "TaskGroups": [
            {
                "Name": "zeppelin",
                "Count": 1,
                "Constraints": null,
                "Tasks": [
                    {
                        "Name": "zeppelin",
                        "Driver": "raw_exec",
                        "User": "",
                        "Config": {
                            "command": "zeppelin-docker-wrapper"
                        },
                        "Constraints": null,
                        "Env": {
                            "ZEPPELIN_CONTAINER_NAME": "{{id}}",
                            "ZEPPELIN_NOTEBOOK_MOUNT": "${NOMAD_TASK_DIR}/notebooks",
                            "ZEPPELIN_SPARK_MASTER" : "local[*]"
                        },
                        "Services": [
                            {
                                "Id": "",
                                "Name": "{{id}}-ui",
                                "Tags": null,
                                "PortLabel": "ui",
                                "Checks": [
                                    {
                                        "Id": "",
                                        "Name": "service: \"{{id}}-ui\" check",
                                        "Type": "http",
                                        "Command": "",
                                        "Args": null,
                                        "Path": "/",
                                        "Protocol": "",
                                        "Interval": 10000000000,
                                        "Timeout": 2000000000
                                    }
                                ]
                            }
                        ],
                        "Resources": {
                            "CPU": 500,
                            "MemoryMB": 512,
                            "DiskMB": 300,
                            "IOPS": 0,
                            "Networks": [
                                {
                                    "Public": false,
                                    "CIDR": "",
                                    "ReservedPorts": null,
                                    "DynamicPorts": [
                                        {
                                            "Label": "ui",
                                            "Value": 0
                                        }
                                    ],
                                    "IP": "",
                                    "MBits": 10
                                }
                            ]
                        },
                        "Meta": null,
                        "KillTimeout": 20000000000,
                        "LogConfig": {
                            "MaxFiles": 10,
                            "MaxFileSizeMB": 10
                        },
                        "Artifacts": [
                            {
                                "GetterSource": "https://raw.githubusercontent.com/FRosner/docker-zeppelin/1.0.0/zeppelin-docker-wrapper",
                                "GetterOptions": {},
                                "RelativeDest": "local/"
                            }
                        ]
                    }
                ],
                "RestartPolicy": {
                    "Interval": 60000000000,
                    "Attempts": 2,
                    "Delay": 15000000000,
                    "Mode": "delay"
                },
                "Meta": null
            }
        ],
        "Update": {
            "Stagger": 10000000000,
            "MaxParallel": 1
        },
        "Periodic": null,
        "Meta": null,
        "Status": "",
        "StatusDescription": "",
        "CreateIndex": 0,
        "ModifyIndex": 0
    }
}
